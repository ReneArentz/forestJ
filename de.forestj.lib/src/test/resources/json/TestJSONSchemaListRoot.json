{
	"$id": "https://example.com/define.schema.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "An example of handling a complex class structure with JSON",
	
	"type": "array",
	"mapping": "java.util.List",
	"items": {
		"ShipOrderRef": {
			"type": "object",
			"mapping": "de.forestj.lib.test.io.Data$ShipOrder",
			"properties": {
				"OrderIdJSON" : {
					"type": "string",
					"mapping": "OrderId"
				},
				"OrderPersonJSON" : {
					"type": "string",
					"mapping": "OrderPerson"
				},
				"OrderDateJSON" : {
					"type": "string",
					"mapping": "OrderDate"
				},
				"OverallPriceJSON" : {
					"type": "number",
					"mapping": "OverallPrice"
				},
				"ShipToJSON": {
					"type": "object",
					"mapping": "ShipTo:de.forestj.lib.test.io.Data$ShipTo",
					"properties": {
						"NameJSON" : {
							"type": "string",
							"mapping": "Name"
						},
						"StreetJSON" : {
							"type": "string",
							"mapping": "Street"
						},
						"NumberJSON" : {
							"type": "integer",
							"mapping": "Number"
						},
						"CityJSON" : {
							"type": "string",
							"mapping": "City"
						},
						"CountryJSON" : {
							"type": "string",
							"mapping": "Country"
						},
						"DeliveredJSON" : {
							"type": "string",
							"mapping": "Delivered"
						},
						"StoredJSON" : {
							"type": "string",
							"mapping": "Stored"
						},
						"HighPriorityJSON" : {
							"type": "boolean",
							"mapping": "HighPriority"
						}
					}
				},
				"ShipMoreInfoJSON": {
					"type": "object",
					"mapping": "ShipMoreInfo:de.forestj.lib.test.io.Data$ShipMoreInfo",
					"properties": {
						"MoreNoteJSON" : {
							"type": "string",
							"mapping": "MoreNote"
						},
						"ShipFromJSON": {
							"type": "object",
							"mapping": "ShipFrom:de.forestj.lib.test.io.Data$ShipFrom",
							"properties": {
								"DeliveredBy" : {
									"type": "string",
									"mapping": "DeliveredBy"
								},
								"DeliveredCountry" : {
									"type": "string",
									"mapping": "DeliveredCountry"
								},
								"ShipVia" : {
									"type": "integer",
									"mapping": "ShipVia"
								},
								"ShipRegistered" : {
									"type": "string",
									"mapping": "ShipRegistered"
								}
							}
						},
						"ShipSiteJSON": {
							"type": "object",
							"mapping": "ShipSite:de.forestj.lib.test.io.Data$ShipSite",
							"properties": {
								"SiteName" : {
									"type": "string",
									"mapping": "SiteName"
								},
								"Building" : {
									"type": "string",
									"mapping": "Building"
								}
							}
						},
						"ShipLocationJSON": {
							"type": "object",
							"mapping": "ShipLocation:de.forestj.lib.test.io.Data$ShipLocation",
							"properties": {
								"LocName" : {
									"type": "string",
									"mapping": "LocName"
								},
								"LocFloor" : {
									"type": "integer",
									"mapping": "LocFloor"
								},
								"LocRack" : {
									"type": "integer",
									"mapping": "LocRack",
									"minimum": 0,
									"maximum": 100
								}
							}
						}
					}
				},
				"SomeBoolsJSON" : {
					"type": "array",
					"mapping": "SomeBools",
					"items" : {
						"type": "boolean[]"
					}
				},
				"ShipItemsJSON": {
					"type": "array",
					"mapping": "ShipItems:java.util.List",
					"items": {
						"ShipItemRef": {
							"type": "object",
							"mapping": "de.forestj.lib.test.io.Data$ShipItem",
							"properties": {
								"TitleJSON" : {
									"type": "string",
									"mapping": "Title"
								},
								"NoteJSON" : {
									"type": "string",
									"mapping": "Note",
									"minLength": 4,
									"maxLength": 100
								},
								"ManufacturedTimeJSON" : {
									"type": "string",
									"mapping": "ManufacturedTime"
								},
								"QuantityJSON" : {
									"type": "integer",
									"mapping": "Quantity"
								},
								"PriceJSON" : {
									"type": "number",
									"mapping": "Price"
								},
								"CurrencyJSON" : {
									"type": "string",
									"mapping": "Currency",
									"pattern": "EUR|USD|GBP"
								},
								"SkontoJSON" : {
									"type": "number",
									"mapping": "Skonto"
								},
								"SomeDecimalsJSON" : {
									"type": "array",
									"mapping": "SomeDecimals",
									"items" : {
										"type": "number[]"
									}
								},
								"ShipItemInfoJSON": {
									"type": "object",
									"mapping": "ShipItemInfo:de.forestj.lib.test.io.Data$ShipItemInfo",
									"properties": {
										"DevelopmentJSON" : {
											"type": "string",
											"mapping": "Development"
										},
										"ConstructionJSON" : {
											"type": "string",
											"mapping": "Construction"
										},
										"ImplementationJSON" : {
											"type": "string",
											"mapping": "Implementation"
										}
									}
								}
							}
						}
					},
					"minItems": 0,
					"maxItems": 5
				},
				"required": ["OrderIdJSON"]
			}
		}
	},
	"maxItems": 10
}